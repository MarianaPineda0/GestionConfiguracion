{
    "version": 2,
    "build": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }   
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/main.py"
        }
    ],
    "env":{
        "APP_MODULE":"main:app"
    }
}